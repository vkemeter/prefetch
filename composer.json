{
    "name": "supseven/prefetch",
    "description": "Configure Pages to be prefetched/prerendered with the speculation js api",
    "type": "typo3-cms-extension",
    "version": "1.0.0",
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "php": "^8.2",
        "typo3/cms-core": "^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Supseven\\Prefetch\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "prefetch"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
